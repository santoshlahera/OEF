    function DateFormatValidation() {
        
        //Get the date from the TextBox.
        var dateString = document.getElementById("DateofBirth").value;
        //var regex = /^(((((((0?[])|(1[02]))[\.\-/]?((0?[1-9])|([12]\d)|(3[01])))|(((0?[469])|(11))[\.\-/]?((0?[1-9])|([12]\d)|(30)))|((0?2)[\.\-/]?((0?[1-9])|(1\d)|(2[0-8]))))[\.\-/]?((\d{2})?([\d][\d]))))|((0?2)[\.\-/]?(29)[\.\-/]?(((19)|(20))?(([02468][048])|([13579][26])))))$/;
        var regex = /^(0[1-9]|1[012])[- /.](0[1-9]|[12][0-9]|3[01])[- /.](19|20)\d\d$/;

        //Check whether valid dd/MM/yyyy Date Format.
        var parts = dateString.split("-");
        var dtDOB = new Date(parts[0] + "-" + parts[1] + "-" + parts[2]);
        var day = parts[1];
        var month = parts[0];
        var year = parts[2];
        var dtCurrent = new Date();
        if ((month == 04 || month == 06 || month == 09 || month == 11) && day == 31) {
            $(".DateofBirthEror").show();
            $("#DateofBirth").css('border-color', 'red');
            $(".DateofBirthEror").text("Please enter the valid date.");
            $('#DateofBirth').val("");
            return false;
        }
        if (month == 1 || month == 3 || month == 5 || month == 7 || month == 8 || month == 10 || month == 12) {
            //if ((0 == year % 4) && (0 != year % 100) || (0 == year % 400)) {
            //    $(".DateofBirthEror").show();
            //    $("#DateofBirth").css('border-color', 'red');
            //    $(".DateofBirthEror").text("Please enter date of birth in mm-dd-yyyy format.");
            //    $('#DateofBirth').val("");
            //    return false;
            // }
        }
        if (month == 2) { // check for february 29th
            var isleap = (year % 4 == 0 && (year % 100 != 0 || year % 400 == 0));
            if (day > 29 || (day == 29 && !isleap)) {
                $(".DateofBirthEror").show();
                $("#DateofBirth").css('border-color', 'red');
                $(".DateofBirthEror").text("Wrong Date.");
                $('#DateofBirth').val("");
                return false;
            }
        }

        if (day == "00") {
            $(".DateofBirthEror").show();
            $("#DateofBirth").css('border-color', 'red');
            $(".DateofBirthEror").text("Please enter in mm-dd-yyyy format.");
            $('#DateofBirth').val("");
            return false;
        }
        if (month == "00") {
            $(".DateofBirthEror").show();
            $("#DateofBirth").css('border-color', 'red');
            $(".DateofBirthEror").text("Please enter in mm-dd-yyyy format.");
            $('#DateofBirth').val("");
            return false;
        }
        if (regex.test(dateString)) {

            if (dateString.charAt(2) != "-") {
                $(".DateofBirthEror").show();
                $("#DateofBirth").css('border-color', 'red');
                $(".DateofBirthEror").text("Please enter in mm-dd-yyyy format.");
                $('#DateofBirth').val("");
                return false;
            }
            if (dateString.charAt(5) != "-") {
                $('#DateofBirth').val("");
                return false;
            }


            //lblError.innerHTML = "Eligibility 18 years ONLY."
            if (dtCurrent.getFullYear() - dtDOB.getFullYear() < 18) {
                $(".DateofBirthEror").show();
                $("#DateofBirth").css('border-color', 'red');
                $(".DateofBirthEror").text("You must be at least 18 years old to sign up for electricity service.");
                $('#DateofBirth').val("");
                return false;

            }

            if (dtCurrent.getFullYear() - dtDOB.getFullYear() == 18) {

                //CD: 11/06/2018 and DB: 15/07/2000. Will turned 18 on 15/07/2018.
                if (dtCurrent.getMonth() < dtDOB.getMonth()) {
                    $(".DateofBirthEror").show();
                    $("#DateofBirth").css('border-color', 'red');
                    $(".DateofBirthEror").text("You must be at least 18 years old to sign up for electricity service.");
                    $('#DateofBirth').val("");
                    return false;
                }
                if (dtCurrent.getMonth() == dtDOB.getMonth()) {
                    //CD: 11/06/2018 and DB: 15/06/2000. Will turned 18 on 15/06/2018.
                    if (dtCurrent.getDate() < dtDOB.getDate()) {
                        $(".DateofBirthEror").show();
                        $("#DateofBirth").css('border-color', 'red');
                        $(".DateofBirthEror").text("Please enter date of birth in mm-dd-yyyy format.");
                        $('#DateofBirth').val("");
                        return false;
                    }
                }
            }

            if (day.length < 2 || (month.length < 2)) {
                $(".DateofBirthEror").show();
                $(".DateofBirthEror").text("Please enter date of birth in mm-dd-yyyy format.");
                $("#DateofBirth").css('border-color', 'red');
                $('#DateofBirth').val("");
            }
            else {
                $(".DateofBirthEror").hide();
                $("#DateofBirth").css('border-color', '');
                // return true;
            }
            if (month.length < 2) {
                $(".DateofBirthEror").show();
                $(".DateofBirthEror").text("Please enter date of birth in mm-dd-yyyy format.");
                $("#DateofBirth").css('border-color', 'red');
