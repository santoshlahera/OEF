    function available(date) {
        var aD = new Array();
        availableDates = availableDates.replace(/^,|,$/g, '');
        aD = availableDates.toString().split(',');
        var month = (date.getMonth() + 1);
        if (month < 10) {
            month = "0" + month;
        }
        var dd = date.getDate();
        if (dd < 10) {
            dd = "0" + dd;
        }
        dmy = dd + "-" + month + "-" + date.getFullYear();
        if ($.inArray(dmy, aD) != -1) {

            return [true, "", "Available"];
        }
        else {
            return [false, "", "unAvailable"];
        }
    }
